{
  "name": "gtavbrowser-mcp",
  "version": "0.2.1",
  "description": "Model Context Protocol (MCP) server for browsing and extracting files from Grand Theft Auto V's RPF archives",
  "license": "MIT",
  "author": "Herwi",
  "repository": {
    "type": "git",
    "url": "https://github.com/Herwi/gtavbrowser-mcp.git"
  },
  "homepage": "https://github.com/Herwi/gtavbrowser-mcp",
  "keywords": [
    "mcp",
    "gta5",
    "gtav",
    "rpf",
    "model-context-protocol",
    "claude",
    "ai",
    "assistant",
    "grand-theft-auto"
  ],
  "type": "module",
  "bin": {
    "gtavbrowser-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "scripts": {
    "build": "tsc && shx cp src/magic.dat dist/magic.dat && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "test": "node test/test-basic.js",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@types/node": "^22",
    "shx": "^0.3.4",
    "typescript": "^5.8.2"
  }
}